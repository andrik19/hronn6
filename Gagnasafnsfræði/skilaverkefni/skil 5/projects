
SELECT 'projects: id --> pid' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT c.id
    FROM projects c
    GROUP BY c.id
    HAVING COUNT(DISTINCT c.pid) > 1
) X;


SELECT 'projects: id --> sid' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT c.id
    FROM projects c
    GROUP BY c.id
    HAVING COUNT(DISTINCT c.sid) > 1
) X;


