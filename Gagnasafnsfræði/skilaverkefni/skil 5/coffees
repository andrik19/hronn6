
SELECT 'coffees: did --> hid' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT c.did
    FROM coffees c
    GROUP BY c.did
    HAVING COUNT(DISTINCT c.hid) > 1
) X;


SELECT 'coffees: did --> cid' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT c.did
    FROM coffees c
    GROUP BY c.did
    HAVING COUNT(DISTINCT c.cid) > 1
) X;


