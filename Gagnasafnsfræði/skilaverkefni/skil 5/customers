
SELECT 'customers: cid --> cn' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT c.cid
    FROM customers c
    GROUP BY c.cid
    HAVING COUNT(DISTINCT c.cn) > 1
) X;


