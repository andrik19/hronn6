
SELECT 'boats: bl --> bno' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT b.bl
    FROM boats b
    GROUP BY b.bl
    HAVING COUNT(DISTINCT b.bno) > 1
) X;


SELECT 'boats: bl --> z' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
